Package ASL4RPALibraries
    
System UIExcel :Application  [
	description "Excel is an library for excel rpa operations"]  
   
RobotOperationType OpenExcel("filePath")
RobotOperationType SaveExcel("filePath")

RobotOperationType CreateExcelWorksheet("sheetName")

RobotOperationType PutStringToCell("sheetName","column","row","stringValue") 
RobotOperationType PutNumberToCell("sheetName","column","row","numberValue") 
RobotOperationType PutDateToCell("sheetName","column","row","DateValue") 

RobotOperationType CheckCellType("sheetName","column","row") 

RobotOperationType GetColumnCount("sheetName") 
RobotOperationType GetColumnValues("sheetName","column")
RobotOperationType GetSheetNames("excelFilePath") 

RobotOperationType ReadCellDataByCoordinates("sheetName","column","line")  
RobotOperationType ReadCellDataByName("sheetName","columnNameAndLine")  
RobotOperationType ReadCellDataByColumnName("sheetName","columnName","filePath","item")  

RobotOperationType LoadRangeToDataTable("filePath","sheetName", output "dataTable") 
   
 